{
  "name": "theme-bundler",
  "version": "1.0.0",
  "description": "Theme frontend asset bundler setup",
  "main": "index.js",
  "scripts": {
    "build:dev": "cross-env webpack --hide-modules --colors --mode=development --env.development",
    "build:dev:clean": "npm run clean:before && cross-env webpack --hide-modules --colors --mode=development --env.development",
    "build:prod": "cross-env webpack --hide-modules --colors --mode=production --env.production",
    "build:prod:clean": "npm run clean:before && cross-env webpack --hide-modules --colors --mode=production --env.production",
    "build:prod:analyze": "npm run clean:before && cross-env webpack --hide-modules --colors --mode=production --env.production --env.analyze",
    "start:dev": "cross-env webpack --mode=development --env.development --watch",
    "start:dev:clean": "npm run clean:before && cross-env webpack --mode=development --env.development --watch",
    "start:dev:browsersync": "npm run clean:before && cross-env webpack --mode=development --env.development --env.browsersync --watch",
    "clean:before": "rm -rf ../dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/magnussmed/tinder-for-twitch.git"
  },
